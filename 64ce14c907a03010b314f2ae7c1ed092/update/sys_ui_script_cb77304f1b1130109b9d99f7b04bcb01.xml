        <script><![CDATA[angular.module('angularapp', []);
angular.module('angularapp').controller('AngularAppCtrl', function ($scope,$location) {

	$scope.myVar = '';
	$scope.i = 0;
	$scope.clearTxt = function () {
		$scope.myVar = '';
		$scope.myUrl = $location.absUrl();
		$scope.i++;
		alert("The myVar value was cleared" + $scope.i +  " times by " + g_user.userName + ".Mi url es =>" +$scope.myUrl );
	};
});
]]></script>
